{
  "name": "testinaweb/php-access-all",
  "type": "library",
  "description": "A library to access and to unit test private and protected methods and properties",
  "keywords": ["php", "phpunit", "test private method", "test protected method", "unit test"],
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "Manuel Kanah",
      "email": "manuel@kanah.it",
      "homepage": "http://www.labna.it/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*@dev"
  },
  "autoload": {
    "psr-4": {
      "Testinaweb\\AccessAll\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Testinaweb\\AccessAll\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
